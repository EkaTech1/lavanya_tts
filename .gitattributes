# Track only model files with LFS
Fastspeech2_HS/**/*.pth filter=lfs diff=lfs merge=lfs -text
Fastspeech2_HS/**/*.pt filter=lfs diff=lfs merge=lfs -text
Fastspeech2_HS/**/*.bin filter=lfs diff=lfs merge=lfs -text
Fastspeech2_HS/**/*.ckpt filter=lfs diff=lfs merge=lfs -text

# Don't track regular files with LFS
*.md -text
*.txt -text
*.json -text
*.yaml -text
*.py -text
*.html -text
*.css -text
*.js -text
